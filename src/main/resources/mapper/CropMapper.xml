<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.njfu.wa.sys.mapper.CropMapper">

    <select id="com.njfu.wa.sys.mapper.CropMapper.selectCrops" resultType="Crop">
        SELECT crop_id, crop_name, crop_ps
        FROM crop
    </select>

    <select id="selectCropById" resultType="Crop">
        SELECT crop_id, crop_name
        FROM crop
        WHERE crop_id = #{crop_id}
    </select>
</mapper>	